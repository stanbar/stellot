{
  "name": "stellar-voting",
  "version": "1.0.0",
  "description": "Stellar election smart contract ",
  "main": "dist/main.js",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "start": "tsc && DEBUG=* node -r dotenv/config .",
    "start-ts": "ts-node-dev -r dotenv/config src/main.ts",
    "start:dev": "npm run lint && ts-node-dev -r dotenv/config src/main.ts",
    "test": "jest --watchAll",
    "prettier": "prettier -c --write \"**/*\"",
    "format-imports": "import-sort --write '**/*.{js,jsx,ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stasbar/stellar-voting.git"
  },
  "keywords": [
    "stellar",
    "smart",
    "contract",
    "blockchain",
    "voting",
    "election"
  ],
  "author": "Stanislaw Baranski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stasbar/stellar-voting/issues"
  },
  "homepage": "https://github.com/stasbar/stellar-voting#readme",
  "dependencies": {
    "bn.js": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "elliptic": "^6.5.2",
    "express": "~4.17.1",
    "fetch": "^1.1.0",
    "http": "0.0.0",
    "http-errors": "^1.7.3",
    "http-status-codes": "latest",
    "morgan": "~1.9.1",
    "prettier": "^1.19.1",
    "request": "^2.88.0",
    "stellar-sdk": "^4.0.2",
    "tweetnacl": "^1.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^6.1.1",
    "@types/elliptic": "^6.4.12",
    "@types/express": "^4.17.1",
    "@types/http-errors": "^1.6.2",
    "@types/jest": "^24.0.16",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.2",
    "@types/supertest": "^2.0.8",
    "@umijs/fabric": "^1.1.10",
    "check-prettier": "^1.0.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.8.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^3.6.3"
  }
}
