{
  "name": "stellar-voting",
  "version": "1.0.0",
  "description": "Stellar election smart contract ",
  "main": "src/main.ts",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "start": "ts-node -r dotenv/config .",
    "start:dev": "ts-node-dev -r dotenv/config src/main.ts",
    "start:prod": "NODE_ENV=production ts-node -r dotenv/config .",
    "test": "npx ava-ts",
    "test:watch": "npx ava-ts --watch --color",
    "prettier": "prettier -c --write \"**/*\"",
    "format-imports": "import-sort --write '**/*.{js,jsx,ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stasbar/stellar-voting.git"
  },
  "keywords": [
    "stellar",
    "smart",
    "contract",
    "blockchain",
    "voting",
    "election"
  ],
  "author": "Stanislaw Baranski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stasbar/stellar-voting/issues"
  },
  "homepage": "https://github.com/stasbar/stellar-voting#readme",
  "dependencies": {
    "@futuretense/ed25519-box": "^1.0.1",
    "@stellot/crypto": "^2.1.1",
    "@stellot/types": "^1.1.10",
    "asn1.js": "^5.4.1",
    "bn.js": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "elgamal": "^0.3.0",
    "elliptic": "^6.5.4",
    "errorhandler": "^1.5.1",
    "express": "~4.18.2",
    "fetch": "^1.1.0",
    "http": "0.0.0",
    "http-errors": "^1.7.3",
    "http-status-codes": "latest",
    "ipfs-http-client": "^44.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "long": "^4.0.0",
    "minimist": ">=1.2.7",
    "moment": "^2.29.4",
    "mongoose": "^5.13.8",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.10.0",
    "node-fetch": "^2.6.7",
    "prettier": "^2.8.1",
    "request": "^2.88.0",
    "slug": "^2.1.1",
    "stellar-sdk": "^8.2.3",
    "uuidv4": "^6.2.13",
    "winston": "^3.8.2"
